# Roots for synchronization
root    = /Users/evansenter/Data
root    = ssh://evansenter@bc-clavius/Data
sshargs = -C

# paths to synchronize
path = *

# Ignore some names and paths
ignore = Name .*
ignore = Name *.so
ignore = Name *.o
ignore = Name *.a
ignore = Name *.out
ignore = Path Kinetics/sprinzl_trna/2009_sequences/output/rnasubopt/*

# keep log
log = false

# skip asking for confirmations for non-conflicting changes
auto = true

# Go fast
fastcheck = true

copythreshold = 100000
